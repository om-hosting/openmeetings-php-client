{
    "name": "om-hosting/openmeetings-php-client",
    "description": "OpenMeetings PHP package to expose OpenMeetings Rest API - see https://openmeetings.apache.org/swagger/",
    "keywords": [
        "openmeetings",
        "swagger",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "https://github.com/om-hosting/openmeetings-php-client",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Sebastian Wagner",
            "homepage": "https://om-hosting.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~2.18"
    },
    "autoload": {
        "psr-4": { "Swagger\\Client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Swagger\\Client\\" : "test/" }
    }
}
